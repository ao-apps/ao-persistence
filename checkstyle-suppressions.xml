<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="MissingJavadocMethod" />

  <!-- Consistency with standard "GZIPInputStream" and "GZIPOutputStream" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]GZIPSerializer\.java$"
    message="'GZIPSerializer'"
  />

  <!-- Will not reorder overloads -->
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]AbstractPersistentBuffer\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]DynamicPersistentBlockBuffer\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]LargeMappedPersistentBuffer\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]MappedPersistentBuffer\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]PersistentBuffer\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]PersistentLinkedList\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]persistence[/\\]TwoCopyBarrierBuffer\.java$"
  />

</suppressions>
